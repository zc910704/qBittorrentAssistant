<Application x:Class="qBittorrentAssistant.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:qBittorrentAssistant"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="Language_zh" Source="/Resource/LanguageResource/LanguageResource_zh_CN.xaml"></ResourceDictionary>
                <ResourceDictionary x:Name="Language" Source="/Resource/Converter/Converter.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Resource/Style/ListViewStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>        
    </Application.Resources>
</Application>
